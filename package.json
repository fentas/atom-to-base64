{
  "name": "to-base64",
  "main": "./lib/to-base64",
  "version": "0.5.1",
  "description": "Converts any data (also https? content) to base64, furthermore it creates different views like Data URI if possible.",
  "activationCommands": {
    "atom-text-editor": [
      "to-base64:encode",
      "to-base64:decode",
      "to-base64:insert"
    ],
    "atom-workspace": [
      "to-base64:view"
    ]
  },
  "repository": "https://github.com/fentas/atom-to-base64",
  "license": "AGPL-3.0",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "coffee-script": "^1.11.1",
    "fs-plus": "^2.9.3",
    "image-size": "^0.5.0",
    "mime": "^1.3.4",
    "minimatch": "^3.0.3",
    "season": "^5.4.1"
  },
  "author": {
    "name": "Jan Guth",
    "email": "jan.guth@gmail.com",
    "url": "jan.guth.so"
  },
  "keywords": [
    "convert",
    "base64",
    "font",
    "file",
    "image",
    "css",
    "web resources"
  ]
}
